    "admin-ngnuggetsdotcom": {
      "projectType": "application",
      "root": "apps/admin-ngnuggetsdotcom",
      "sourceRoot": "apps/admin-ngnuggetsdotcom/src",
      "prefix": "stevewitmanangular",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/admin-ngnuggetsdotcom",
            "index": "apps/admin-ngnuggetsdotcom/src/index.html",
            "main": "apps/admin-ngnuggetsdotcom/src/main.ts",
            "polyfills": "apps/admin-ngnuggetsdotcom/src/polyfills.ts",
            "tsConfig": "apps/admin-ngnuggetsdotcom/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "apps/admin-ngnuggetsdotcom/src/favicon.ico",
              "apps/admin-ngnuggetsdotcom/src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "apps/admin-ngnuggetsdotcom/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/admin-ngnuggetsdotcom/src/environments/environment.ts",
                  "with": "apps/admin-ngnuggetsdotcom/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "admin-ngnuggetsdotcom:build:production"
            },
            "development": {
              "browserTarget": "admin-ngnuggetsdotcom:build:development",
              "port": 4201
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "admin-ngnuggetsdotcom:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/admin-ngnuggetsdotcom/src/**/*.ts",
              "apps/admin-ngnuggetsdotcom/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/admin-ngnuggetsdotcom"],
          "options": {
            "jestConfig": "apps/admin-ngnuggetsdotcom/jest.config.js",
            "passWithNoTests": true
          }
        }
